<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
    <context-param>
        <param-name>contextConfigLocation</param-name>
        <param-value>/WEB-INF/applicationContext.xml</param-value>
    </context-param>
    <!-- WebLogic cluster -->
    <filter>
        <filter-name>pssProxyFilter</filter-name>
        <filter-class>com.precise.ifweb.Collection.Filters.proxyFilter.ProxyServerSideFilter</filter-class>
        <init-param>
            <param-name>PssI3Root</param-name>
            <param-value>C:\Precise</param-value>
        </init-param>
        <init-param>
            <param-name>PssClusterName</param-name>
            <param-value>CL_CarRentalWeb_WL</param-value>
        </init-param>
    </filter>
    <filter>
        <filter-name>instrProxyFilter</filter-name>
        <filter-class>com.precise.ifweb.Collection.Filters.proxyFilter.ProxyInstrumentationFilter</filter-class>
        <init-param>
            <param-name>PssI3Root</param-name>
            <param-value>C:\Precise</param-value>
        </init-param>
        <init-param>
            <param-name>PssClusterName</param-name>
            <param-value>CL_CarRentalWeb_WL</param-value>
        </init-param>
    </filter>
    <filter-mapping>
        <filter-name>pssProxyFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <filter-mapping>
        <filter-name>instrProxyFilter</filter-name>
        <url-pattern>/*</url-pattern>
    </filter-mapping>
    <!-- <listener>
        <listener-class>org.springframework.web.context.ContextLoaderListener</listener-class>
    </listener>
    <servlet>
        <servlet-name>dispatcher</servlet-name>
        <servlet-class>org.springframework.web.servlet.DispatcherServlet</servlet-class>
        <load-on-startup>2</load-on-startup>
    </servlet>-->
    <servlet>
        <servlet-name>ListCars</servlet-name>
        <servlet-class>carrental.ListCarsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddCar</servlet-name>
        <servlet-class>carrental.AddCarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RentCar</servlet-name>
        <servlet-class>carrental.RentCarServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>TestJDBC</servlet-name>
        <servlet-class>carrental.testers.TestJDBCServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveJDBC</servlet-name>
        <servlet-class>carrental.SaveJDBCServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddCarJDBC</servlet-name>
        <servlet-class>carrental.AddCarJDBCServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ConfigureDatabase</servlet-name>
        <servlet-class>carrental.ConfigureDatabaseServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListCarsJDBC</servlet-name>
        <servlet-class>carrental.ListCarsJDBCServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AddBulkCars</servlet-name>
        <servlet-class>carrental.AddBulkCarsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CreateDatabaseObjects</servlet-name>
        <servlet-class>carrental.CreateDatabaseObjectsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SimulateLoad</servlet-name>
        <servlet-class>carrental.simulate.SimulateLoadServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SaveOperations</servlet-name>
        <servlet-class>carrental.simulate.SaveOperationsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SimulateLoadDisplay</servlet-name>
        <servlet-class>carrental.simulate.SimulateLoadDisplayServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>testservlet</servlet-name>
        <servlet-class>carrental.simulate.testservlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>dispatcher</servlet-name>
        <url-pattern>*.htm</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListCars</servlet-name>
        <url-pattern>/listcars</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddCar</servlet-name>
        <url-pattern>/addcar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RentCar</servlet-name>
        <url-pattern>/rentcar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>TestJDBC</servlet-name>
        <url-pattern>/testjdbc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddCarJDBC</servlet-name>
        <url-pattern>/addcarjdbc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListCarsJDBC</servlet-name>
        <url-pattern>/listcarsjdbc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveJDBC</servlet-name>
        <url-pattern>/savejdbc</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AddBulkCars</servlet-name>
        <url-pattern>/addbulkcars</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ConfigureDatabase</servlet-name>
        <url-pattern>/configuredatabase</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CreateDatabaseObjects</servlet-name>
        <url-pattern>/createdatabaseobjects</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SimulateLoad</servlet-name>
        <url-pattern>/simulateload</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SaveOperations</servlet-name>
        <url-pattern>/saveoperations</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SimulateLoadDisplay</servlet-name>
        <url-pattern>/simulateloaddisplay</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>testservlet</servlet-name>
        <url-pattern>/testservlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>35</session-timeout>
    </session-config>
    <mime-mapping>
        <extension>html</extension>
        <mime-type>text/html</mime-type>
    </mime-mapping>
    <mime-mapping>
        <extension>txt</extension>
        <mime-type>text/plain</mime-type>
    </mime-mapping>
</web-app>